            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">


                        <div class="section__content">
                            <div class="container">
                                <div class="row row--center">
                                    <div class="col-lg-6 col-md-8 u-s-m-b-30">
                                        <div class="l-f-o">
                                            <div class="l-f-o__pad-box">
                                                <h1 class="gl-h1">ADD COUPEN</h1>
                                                <form action="/admin/addcoupen" method="POST" class="l-f-o__form"
                                                    id="registerform" name="registerform">
                                                    <div class="u-s-m-b-30">
                                                        <label class="gl-label" for="reg-fname">COUPEN NAME*</label>
                                                        <input class="input-text input-text--primary-style" type="text"
                                                            id="userfname" name="coupenname" required oninput="nameValid(),this.value = this.value.toUpperCase()"
                                                            placeholder="COUPEN NAME">
                                                            <span style="color: rgb(201, 48, 48);" id="nameReq"></span>
                                                    </div>
                                                    <div class="u-s-m-b-30">
                                                        <label class="gl-label" for="reg-lname">COPEN CODE*</label>
                                                        <input class="input-text input-text--primary-style" type="text"
                                                            id="userlname" name="coupencode" oninput="this.value = this.value.toUpperCase()" required 
                                                            placeholder="COUPEN CODE">
                                                            <span style="color: brown;" id="lastnameReq"></span>
                                                    </div>

                                                    <div class="u-s-m-b-30">
                                                        <label class="gl-label" for="reg-password">OFFER PERCENTAGE *</label>
                                                        <input class="input-text input-text--primary-style"
                                                            type="number" id="password" name="coupenvalue" required
                                                            placeholder="OFFER PERCENTAGE">
                                                            <span style="color: brown;" id="passLen"></span>
                                                    </div>
                                                    
                                                    <div class="u-s-m-b-30">
                                                        <label class="gl-label" for="reg-password">Validity</label>
                                                        <input class="input-text input-text--primary-style"
                                                            type="date" id="date" name="validdate" required
                                                            >
                                                            <span style="color: brown;" id="passLen"></span>
                                                    </div>
                                                    <div class="u-s-m-b-15">
                                                        <button class="btn btn--e-transparent-brand-b-2"
                                                            type="submit">CREATE</button>
                                                    </div>
                                             {{#if coupenexist}}
                                            {{!-- <label for="remember-me"
                                                class="label-agree-term"><span><span></span></span>Remember me</label> --}}
                                           
                                            <span class=" " style="color:red;" id="invalid">COUPEN CODE EXIST</span>
                                            {{/if}}

                                                </form>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>







                    </div>
                </div>
            </div>
